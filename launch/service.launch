<launch>
	<arg
		name = "rate"
		default = "5"
	/>
	<node
		pkg = "beginner_tutorials"
		type = "talker"
		name = "publisher"
		output = "screen"
		args="$(arg rate)"
	/>
	<node
		pkg = "beginner_tutorials"
		type = "listener"
		name = "subscriber"
		output = "screen"	
		launch-prefix = "xterm -e"
	/>
</launch>